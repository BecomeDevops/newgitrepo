pipeline {


agent any

  stages {

stage('git clone') {

  steps {

echo "cloning the code repo from github"
    
  }

  
}

    stage('Build') {
   steps {
    echo "build the code and creating a jar file"

     
   }

      
    }


    stage('test') {

steps {

echo "testing the code"
  
}
      
    }

    stage('deploy') {

steps {

echo "deploying the code"
  
}

      
    }

    


    
  }




  
}
